# Makefile Test (no extension)

.PHONY: test clean

test:
	@echo "Testing extensionless file processing"
	@echo "This Makefile has no file extension"

clean:
	@echo "Clean target"

help:
	@echo "Available targets:"
	@echo "  test  - Run tests"
	@echo "  clean - Clean build artifacts"
	@echo "  help  - Show this help"
